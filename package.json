{
  "name": "ai-gallery",
  "displayName": "AI Gallery",
  "publisher": "ai-gallery",
  "description": "",
  "version": "0.0.8",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "browser": "./dist/web/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ai-gallery.helloWorld",
        "title": "HelloGallery"
      },
      {
        "command": "ai-gallery.executePrompt",
        "title": "ExecutePrompt"
      }
    ]
  },
  "scripts": {
    "test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
    "pretest": "npm run compile-web",
    "vscode:prepublish": "npm run package-web",
    "compile-web": "webpack",
    "watch-web": "webpack --watch",
    "package-web": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. .",
    "host": "serve --ssl-cert localhost+1.pem --ssl-key localhost+1-key.pem --cors -l 3002",
    "build": "npm run compile-web"
  },
  "devDependencies": {
    "@types/assert": "^1.5.10",
    "@types/mocha": "^10.0.6",
    "@types/vscode": "^1.98.0",
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vscode/test-web": "^0.0.52",
    "assert": "^2.1.0",
    "eslint": "^8.56.0",
    "mocha": "^10.3.0",
    "process": "^0.11.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@vscode/extension-telemetry": "^0.9.8",
    "async-lz-string": "^1.1.0"
  }
}
